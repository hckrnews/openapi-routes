{
  "name": "@ponbike/openapi-routes",
  "version": "2.0.0",
  "description": "OpenAPI routes",
  "files": [
    "src/api.js",
    "dist/api.js",
    "dist/api.js.map",
    "dist/api.module.js",
    "dist/api.module.js.map",
    "dist/api.modern.js",
    "dist/api.modern.js.map",
    "dist/api.umd.js",
    "dist/api.umd.js.map"
  ],
  "source": "src/api.js",
  "main": "dist/api.js",
  "module": "dist/api.module.js",
  "esmodule": "dist/api.modern.js",
  "unpkg": "dist/api.umd.js",
  "umd:main": "dist/api.umd.js",
  "exports": {
    ".": {
      "require": "./dist/api.js",
      "default": "./dist/api.module.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ponbike/openapi-routes.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "keywords": [
    "javascript",
    "express",
    "callback"
  ],
  "author": {
    "name": "Pieter Wigboldus",
    "url": "https://www.gazelle.nl/"
  },
  "bugs": {
    "url": "https://github.com/ponbike/openapi-routes/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/ponbike/openapi-routes#readme",
  "scripts": {
    "build": "microbundle --target node",
    "dev": "microbundle --target node watch",
    "lint": "standard src",
    "lint:fix": "standard src --fix",
    "test": "jest",
    "test:watch": "jest src --watch",
    "coveralls": "jest && codecov && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "codecov": "^3.8.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.22.0",
    "esm": "^3.2.25",
    "jest": "^26.6.3",
    "microbundle": "^0.13.0",
    "prettier": "^2.2.1",
    "standard": "^16.0.3"
  },
  "type": "module",
  "engines": {
    "node": ">= 14"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "describe",
      "it",
      "expect",
      "btoa"
    ],
    "ignore": [
      "**/__fixtures__/"
    ]
  }
}
